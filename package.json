{
  "name": "vue3-filter",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 5173 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^11.0.3",
    "element-plus": "^2.8.2",
    "pinia": "^2.2.2",
    "vue": "^3.5.4",
    "vue-draggable-plus": "^0.5.3",
    "vue-router": "^4.4.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.6.0",
    "@iconify-json/carbon": "^1.2.1",
    "@types/node": "^20.0.0",
    "@unocss/eslint-config": "^0.62.3",
    "@unocss/reset": "^0.62.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue-macros/volar": "^0.29.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.10.0",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.10.0",
    "sass": "^1.78.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.8",
    "typescript": "^5.6.2",
    "unocss": "^0.62.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "^2.11.12",
    "vite": "^5.4.4",
    "vite-plugin-pages": "^0.32.3",
    "vitest": "^2.1.0",
    "vue-tsc": "^2.1.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
